{
  "name": "gti525-fullstack",
  "private": true,
  "version": "0.0.0",
  "main": "config.js",
  "scripts": {
    "server": "nodemon server.js --ignore 'client/' ",
    "client": "npm run dev --prefix client",
    "preview": "npm run preview --prefix client",
    "vite": "npm run client",
    "vue-dev": "npm run client",
    "dev": "concurrently \"npm run server\" \"npm run client\"",
    "start": "node server.js",
    "start-windows": "(for /F \"tokens=*\" %i in (.env) do set %i) && npm start",
    "start-linux": "export $(cat .env | xargs) && npm start",
    "build": "npm run build --prefix ./client",
    "install:server": "npm install",
    "install:client": "cd ./client && npm install",
    "install-full": "npm run install:server && npm run install:client",
    "install-all": "npm run install:server && npm run install:client",
    "lint": "npm run lint --prefix client",
    "lint-fix": "npm run lint-fix --prefix client"
  },
  "dependencies": {
    "body-parser": "^1.19.1",
    "cors": "^2.8.5",
    "express": "^4.17.2",
    "express-rate-limit": "^5.5.1",
    "mongoose": "^6.1.2",
    "morgan": "^1.10.0"
  },
  "devDependencies": {
    "concurrently": "^6.5.0",
    "nodemon": "^2.0.15",
    "eslint": "^6.7.2",
    "eslint-plugin-vue": "^7.0.0",
    "babel-eslint": "^10.1.0"
  }
}
